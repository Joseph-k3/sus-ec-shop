{
  "version": 2,
  "builds": [
    {
      "src": "./index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
    "VITE_SQUARE_APPLICATION_ID": "@vite_square_application_id",
    "VITE_SQUARE_LOCATION_ID": "@vite_square_location_id",
    "VITE_SQUARE_ACCESS_TOKEN": "@vite_square_access_token",
    "VITE_POSTMARK_API_KEY": "@vite_postmark_api_key",
    "VITE_SENDER_EMAIL": "@vite_sender_email",
    "VITE_SITE_PASSWORD": "@vite_site_password"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install"
}
